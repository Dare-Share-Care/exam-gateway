{
  "Routes": [
    {
      "UpstreamPathTemplate": "/api/Order",
      "UpstreamHttpMethod": [
        "Get",
        "Post",
        "Put"
      ],
      "DownStreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "orderwebapi",
          "Port": "80"
        }
      ],
      "DownstreamPathTemplate": "/api/Order"
    },
    {
      "UpstreamPathTemplate": "/api/Order/{id}",
      "UpstreamHttpMethod": [
        "Get",
        "Post",
        "Put"
      ]
    }
  ],
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:5000"
  }
}